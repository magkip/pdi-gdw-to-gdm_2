    <sql>merge into &#x24;&#x7b;mxns.gdm.db.prc_ctl&#x7d;.&#x24;&#x7b;mxns.gdm.schema.prc_ctl&#x7d;.process_run_control&#xd;&#xa;using &#x28;select &#x27;running&#x27; as run_status, &#x27;&#x3f;&#x27; as run_start_ts, null as run_end_ts, &#x27;&#x24;&#x7b;PROJECT_ID&#x7d;&#x27; as project_id,&#xd;&#xa;&#x27;&#x24;&#x7b;PROCESS_NAME&#x7d;&#x27; as process_name&#x29; x  &#xd;&#xa;on x.project_id &#x3d; &#x24;&#x7b;mxns.gdm.db.prc_ctl&#x7d;.&#x24;&#x7b;mxns.gdm.schema.prc_ctl&#x7d;.process_run_control.project_id&#xd;&#xa;and x.process_name &#x3d; &#x24;&#x7b;mxns.gdm.db.prc_ctl&#x7d;.&#x24;&#x7b;mxns.gdm.schema.prc_ctl&#x7d;.process_run_control.process_name&#xd;&#xa;when matched then update &#xd;&#xa;	set process_run_control.run_status &#x3d; x.run_status,&#xd;&#xa;	process_run_control.run_start_ts &#x3d; x.run_start_ts,&#xd;&#xa;	process_run_control.run_end_ts &#x3d; x.run_end_ts&#xd;&#xa;when not matched then insert &#x28;project_id, process_name, run_status, run_start_ts, run_end_ts&#x29; &#xd;&#xa;	values &#x28;x.project_id, x.process_name, x.run_status, x.run_start_ts, x.run_end_ts&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x3b;</sql>
